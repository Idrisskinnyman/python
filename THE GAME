                     elif brick_y==210 and brick_x>=117 and brick_x<=194:
                         speed_y=0 #wall5
                        
                     elif brick_y==120 and brick_x>=411 and brick_x<=454:
                         speed_y=0 #wall6
                         
                     elif brick_y==480 and brick_x>=81 and brick_x<=159:
                         speed_y=0 #wall 7
                     
                     elif brick_y==361 and brick_x>=161 and brick_x<=241:
                         speed_y=0 #wall 8
                        
                     elif brick_y==320 and brick_x>=161 and brick_x<=204:
                         speed_y=0 #wall 9
                        
                     elif brick_y==410 and brick_x>=311 and brick_x<=354:
                         speed_y=0 #wall 10
                     elif brick_y==440 and brick_x>=611 and brick_x<=689:
                         speed_y=0 #wall 11  
                        
                     elif brick_y==440 and brick_x>=611 and brick_x<=654:
                         speed_y=0 #wall 12

                     elif brick_y==280 and brick_x>=681 and brick_x<=724:
                         speed_y=0 #wall 13
                    
                     elif brick_y==80 and brick_x>=501 and brick_x<=599:
                         speed_y=0 #wall 14
            
                     elif brick_y==80 and brick_x>=131 and brick_x<=174:
                         speed_y=0 #wall 15
           
                     elif brick_y==80 and brick_x>=91 and brick_x<=169:
                         speed_y=0 #wall 16 
                  
                     elif brick_y==260 and brick_x>=261 and brick_x<=304:
                         speed_y=0 #wall 17 

                     elif brick_y==460 and brick_x>=451 and brick_x<=494:
                         speed_y=0 #wall 18

                     elif brick_y==240 and brick_x>=560 and brick_x<=604:
                         speed_y=0 #wall 19
         
                         
                     else:    
                         speed_y=1
                         brick_y = brick_y + speed_y
                         if brick_y==590:
                             speed_y=0 

             elif brick_y == 570: 
                 heal = heal -1
#----This is done so you can regenerate heal when still---#
         elif keys[pygame.K_SPACE]: 
             stamina = True
             if heal!=190:
                 heal = heal + 0.25
                        
         if stamina==True:
            if energy >= 190:
                energy=energy+0
            elif energy!=190:
                energy=energy+0.15
        
         elif stamina==False:
            energy=energy-0.15


         if heal==0 or heal<0:
             in_play = False
             in_loss = True
             
             while in_loss:
                 game_window.fill(GREY)
                 mixer.music.stop()#STOPS THE MUSIC
                 damage_sound = mixer.Sound("lol.mp3")#CHOSES A SOUND EFFECT
                 damage_sound.play()#PLAYS THE SOUND EFFECT

                 #DRAWS A SAD FACE
                 pygame.draw.arc(game_window, BLACK, (90,190,250,300),2*3.14,3.14, 3)
                 pygame.draw.rect(game_window,BLACK, (155,50,3,100))
                 pygame.draw.rect(game_window,BLACK, (265,50,3,100))

                 #TEXT DECLARATIONS AND BLIT
                 over=minifont.render("Game Over !",4,BLACK)
                 game_window.blit(over,(350,400))
                 #TEXT DECLARATIONS AND BLIT
                 text=minifont.render("The Cartesian Plane is Inevitable!!",3,BLACK)
                 game_window.blit(text,(350,450))
                 
                 pygame.display.update()#UPDATES DISPLAY TO SHOW THESE CHANGES

                 
                 in_loss = False#ENDS GAME
                 
                 in_menu = False#ENDS GAME
                 
                 in_play = False#ENDS GAME
                 
                 in_win = False#ENDS GAME


         if coin_count == 20: #Win get 20 coins  
             in_play = False
             in_win = True
             mixer.music.stop()#STOPS THE MUSIC
             win_sound = mixer.Sound("win.mp3")#CHOSES A SOUND EFFECT
             win_sound.play()#PLAYS THE SOUND EFFECT
             while in_win:
                 #CREATES A BLANK CANVAS FOR THE WIN SCREEN
                 game_window.fill(WHITE)
                 
                 over=minifont.render("You Win !",4,BLACK)
                 game_window.blit(over,(350,400))
                 pygame.display.update()

